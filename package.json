{
    "name": "property-management-api",
    "version": "1.1.0",
    "scripts": {
        "start:pm-api": "nest start property-management-api",
        "start:pm-api:dev": "nest start property-management-api --watch",
        "start:analysis": "cd apps/analysis-api && serverless offline",
        "start:analysis:dev": "nest start analysis-api --watch",
        "build:pm-api": "nest build property-management-api",
        "build:analysis": "cd apps/analysis-api && serverless package",
        "deploy:analysis": "cd apps/analysis-api && serverless deploy",
        "lint": "eslint 'apps/**/{src,test}/**/*.ts'",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage"
    },
    "keywords": [
        "property",
        "management",
        "api",
        "prioritizing",
        "message"
    ],
    "author": "mjkuranda",
    "license": "ISC",
    "description": "Property Management API for prioritizing messages from the tenants",
    "workspaces": [
        "apps/*",
        "libs/*"
    ],
    "dependencies": {
        "@nestjs/axios": "^4.0.0",
        "@nestjs/common": "^11.1.2",
        "@nestjs/config": "^4.0.2",
        "@nestjs/core": "^11.1.2",
        "aws-lambda": "^1.0.7",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.7",
        "@nestjs/testing": "^11.1.2",
        "@types/aws-lambda": "^8.10.149",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "8.22.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.7.0",
        "serverless-offline": "^13.9.0",
        "serverless-webpack": "^5.15.1",
        "supertest": "^7.1.1",
        "ts-jest": "^29.3.4",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.3",
        "webpack": "^5.99.9",
        "webpack-node-externals": "^3.0.0"
    }
}
