{
    "name": "property-management-api",
    "version": "1.0.0",
    "scripts": {
        "start:pm-api": "nest start property-management-api",
        "start:pm-api:dev": "nest start property-management-api --watch",
        "start:analysis": "nest start analysis-api",
        "start:analysis:dev": "nest start analysis-api --watch",
        "build:pm-api": "nest build property-management-api",
        "build:analysis": "nest build analysis-api",
        "lint": "eslint 'apps/**/{src,test}/**/*.ts'",
        "lint:fix": "eslint . --ext .ts --fix",
